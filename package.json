{
  "name": "redux-action-logger",
  "version": "0.1.2",
  "description": "A simple, extensible redux middleware system to generate logs from actions",
  "main": "lib/redux-action-logger.js",
  "scripts": {
    "build": "./node_modules/.bin/webpack --env build",
    "dev": "./node_modules/.bin/webpack --progress --colors --watch --env dev",
    "flow": "./node_modules/.bin/flow check",
    "test": "./node_modules/.bin/jest",
    "test:tc": "./node_modules/.bin/jest --coverage",
    "test:watch": "./node_modules/.bin/jest --watch",
    "test:coverage": "./node_modules/.bin/jest --coverage"
  },
  "publishConfig": {
    "registry": "http://npm.thorhudl.com"
  },
  "devDependencies": {
    "babel": "6.5.2",
    "babel-cli": "^6.24.1",
    "babel-core": "6.22.1",
    "babel-eslint": "7.1.1",
    "babel-loader": "6.2.10",
    "babel-plugin-add-module-exports": "0.1.2",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "eslint": "1.7.2",
    "eslint-loader": "1.6.1",
    "flow-bin": "^0.44.2",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^19.0.2",
    "nock": "^9.0.13",
    "webpack": "2.2.1",
    "yargs": "6.6.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hudl/redux-action-logger.git"
  },
  "keywords": [],
  "author": "Brian Kaiser and Jon Dokulil",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hudl/redux-action-logger/issues"
  },
  "homepage": "https://github.com/hudl/redux-action-logger#readme",
  "hudl-ci": {
    "buildCommand": "npm run build",
    "lintCommand": null,
    "testCommand": "npm run test:tc"
  }
}
